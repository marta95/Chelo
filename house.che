(defturtle "house-turtle")

(pen-size! house-turtle 2)
(pen-colour! house-turtle "green")

(loop
  (move-forward! house-turtle 40)
  (rotate-right! 45)
  (move-forward! house-turtle 40)
  (rotate-right! 90)
  (move-forward! house-turtle 40)
  (rotate-right! 45)
  (move-forward! house-turtle 40)
  (rotate-right! 90)
  (move-forward! house-turtle (* 1.41 40))
  (rotate-right! 90)
  (sleep 1500))